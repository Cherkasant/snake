{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "lib": ["DOM", "ES2019"],
    "moduleResolution": "Bundler"
  },
  "include": ["script.ts"],
  "exclude": ["dist", "node_modules"]
}


